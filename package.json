{
    "name": "programmingcompetationtemplate",
    "productName": "programmingcompetationtemplate",
    "version": "1.0.0",
    "description": "just template :)",
    "type": "module",
    "main": "server/js/index.js",
    "scripts": {
        "dev": "node server/js/index.js",
        "build": "tsc -p tsconfig.server.json && tsc -p tsconfig.client.json",
        "style": "npx tailwindcss -i ./client/css/tailwind.css -o ./client/css/tailwindBuild.css --watch",
        "dev-b": "npm run build && npm run dev",
        "dev-fb": "npm run build && npm run style && npm run dev && npx tailwindcss -i ./client/css/tailwind.css -o ./client/css/tailwindBuild.css"
    },
    "keywords": [],
    "author": {
        "name": "Marek Sklář"
    },
    "license": "MIT",
    "dependencies": {
        "express": "^4.19.2",
        "undici-types": "^6.11.1"
    },
    "devDependencies": {
        "@types/express": "^4.17.21",
        "@types/node": "^20.6.4",
        "tailwindcss": "^3.4.3",
        "typescript": "^5.4.5"
    }
}
